# editorconfig.org

# top-most EditorConfig file
root = true

[app/**/*.js]
# Unix-style newlines with a newline ending every file
end_of_line = lf
insert_final_newline = true
# indent with two spaces
indent_style = space
indent_size = 2
# trim trailing and insert newline at end
trim_trailing_whitespace = true
insert_final_newline = true
